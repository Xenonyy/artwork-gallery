{
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "build:profile": "next build --profile",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "check-ts-types": "tsc --noemit",
    "lint": "next lint --dir src/",
    "lint:report": "eslint --ext ts --ext tsx --format json --output-file eslint-report.json src/",
    "prepare": "husky install",
    "messages": "npm run messages:extract && npm run messages:compile",
    "messages:extract": "formatjs extract \"src/**/*.ts*\" --out-file src/locales/temp.json",
    "messages:compile": "formatjs compile \"src/locales/temp.json\" --out-file src/locales/hu.json"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "dependencies": {
    "axios": "0.27.2",
    "clsx": "1.1.1",
    "next": "12.1.6",
    "next-compose-plugins": "2.2.1",
    "next-seo": "5.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "6.0.3",
    "react-query": "3.39.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.1",
    "@commitlint/config-angular": "17.0.0",
    "@formatjs/cli": "5.0.1",
    "@next/bundle-analyzer": "12.1.6",
    "@svgr/webpack": "6.2.1",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.3",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.5.1",
    "@types/node": "16.11.27",
    "@types/react": "17.0.43",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "cross-env": "7.0.3",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-formatjs": "3.1.4",
    "eslint-plugin-jest": "26.1.5",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react-perf": "3.3.1",
    "eslint-plugin-testing-library": "5.5.1",
    "husky": "8.0.1",
    "jest": "27.5.1",
    "jest-sonar": "0.2.12",
    "react-test-renderer": "17.0.2",
    "typescript": "4.7.2"
  }
}
